{
    "default": true,
    "no-bare-urls": false,
    "no-alt-text": false,
    "MD002": {
        "level": 1
    },
    "MD003": {
        "style": "atx"
    },
    "MD004": {
        "style": "sublist"
    },
    "MD007": {
        "indent": 4
    },
    "MD009": {
        "br_spaces": 4,
        "list_item_empty_lines": 4
    },
    "MD013": {
        "code_blocks": false,
        "headings": false
    },
    "MD026": {
        "punctuation": ".,;:"
    },
    "MD033": {
        "allowed_elements": [
            "link",
            "iframe",
            "kbd",
            "br",
            "figure",
            "figcaption",
            "p",
            "img",
            "i",
            "mark",
            "small",
            "abbr",
            "details",
            "summary"
        ]
    }
}
